<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="PlacesAutocompleteTextView">
        <attr name="pacv_googleMapsApiKey" format="string"/>
        <attr name="pacv_adapterClass" format="string"/>
        <attr name="pacv_historyFile" format="string"/>
        <attr name="pacv_languageCode" format="string"/>
        <attr name="pacv_clearEnabled" format="boolean"/>
        <attr name="pacv_resultType" format="enum">
            <enum name="geocode" value="0"/>
            <enum name="address" value="1"/>
            <enum name="establishment" value="2"/>
            <enum name="no_type" value="3"/>
        </attr>
    </declare-styleable>

    <attr name="pacv_placesAutoCompleteTextViewStyle" format="reference"/>
</resources>
